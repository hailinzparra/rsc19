{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "DOM",
      "ES2018"
    ],
    "module": "commonjs",
    "rootDir": "./ts",
    "outDir": "./public/js",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}